        "outDir": "dist",
        "module": "commonjs",
        "strictFunctionTypes": true,
        "noImplicitAny": true,
        "removeComments": true,
        "allowJs": true,
        "downlevelIteration": true
    },
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}